lories\"\n        class=\"elevation-1\"\n      >\n        <template v-slot:top>\n          <v-toolbar\n            flat\n          >\n            <v-toolbar-title>Desserts</v-toolbar-title>\n            <v-divider\n              class=\"mx-4\"\n              inset\n              vertical\n            ></v-divider>\n            <v-btn\n              color=\"primary\"\n              dark\n              class=\"mb-2 ml-5\"\n              @click=\"setItems\"\n            >\n              Upload datatable\n            </v-btn>\n            <v-spacer></v-spacer>\n            <v-dialog\n              v-model=\"dialog\"\n              max-width=\"500px\"\n            >\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-btn\n                  color=\"primary\"\n                  dark\n                  class=\"mb-2\"\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                >\n                  New Item\n                </v-btn>\n              </template>\n              <v-card>\n                <v-card-title>\n                  <span class=\"headline\">{{ formTitle }}</span>\n                </v-card-title>\n\n                <v-card-text>\n                  <v-container>\n                    <v-row>\n                      <v-col\n                        cols=\"12\"\n                        sm=\"6\"\n                      >\n                        <v-text-field\n                          v-model=\"curItem.name\"\n                          label=\"Dessert name\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col\n                        cols=\"12\"\n                        sm=\"6\"\n                      >\n                        <v-text-field\n                          v-model=\"curItem.calories\"\n                          label=\"Calories\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col\n                        cols=\"12\"\n                        sm=\"6\"\n                      >\n                        <v-text-field\n                          v-model=\"curItem.fat\"\n                          label=\"Fat (g)\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col\n                        cols=\"12\"\n                        sm=\"6\"\n                      >\n                        <v-text-field\n                          v-model=\"curItem.carbs\"\n                          label=\"Carbs (g)\"\n                        ></v-text-field>\n                      </v-col>\n                      <v-col\n                        cols=\"12\"\n                        sm=\"6\"\n                      >\n                        <v-text-field\n                          v-model=\"curItem.protein\"\n                          label=\"Protein (g)\"\n                        ></v-text-field>\n                      </v-col>\n                    </v-row>\n                  </v-container>\n                </v-card-text>\n\n                <v-card-actions>\n                  <v-spacer></v-spacer>\n                  <v-btn\n                    color=\"blue darken-1\"\n                    text\n                    @click=\"hideModalM(false)\"\n                  >\n                    Cancel\n                  </v-btn>\n                  <v-btn\n                    color=\"blue darken-1\"\n                    text\n                    @click=\"saveNewItem(curItem)\"\n                  >\n                    Save\n                  </v-btn>\n                </v-card-actions>\n              </v-card>\n            </v-dialog>\n            <v-dialog\n              v-model=\"dialogDelete\"\n              max-width=\"500px\"\n            >\n              <v-card>\n                <v-card-title class=\"headline\">Are you sure you want to delete this item?</v-card-title>\n                <v-card-actions>\n                  <v-spacer></v-spacer>\n                  <v-btn\n                    color=\"blue darken-1\"\n                    text\n                    @click=\"hideModalDelM(false)\"\n                  >Cancel</v-btn>\n                  <v-btn\n                    color=\"blue darken-1\"\n                    text\n                    @click=\"deleteItemConfirm\"\n                  >OK</v-btn>\n                  <v-spacer></v-spacer>\n                </v-card-actions>\n              </v-card>\n            </v-dialog>\n          </v-toolbar>\n        </template>\n        <template v-slot:item.actions=\"{ item }\">\n            <v-icon\n            small\n            class=\"mr-2\"\n            @click=\"openDialog(item)\"\n          >\n            mdi-pencil\n          </v-icon>\n          <v-icon\n            small\n            @click=\"deleteItem(item)\"\n          >\n            mdi-delete\n          </v-icon>\n        </template>\n      </v-data-table>\n    </base-material-card>\n  </v-container>\n</template>\n<script>\n  import Vue from 'vue'\n  import axios from 'axios'\n  import VueAxios from 'vue-axios'\n  import Vuex, { mapState, mapActions, mapGetters, mapMutations } from 'vuex'\n  import { mapFields } from '@/js/update_form.js'\n  Vue.use(Vuex, VueAxios, axios)\n  export default {\n    name: 'RegularTables',\n    data: () => ({\n      dialog: false,\n      dialogDelete: false,\n      headers: [\n        {\n          text: 'Dessert (100g serving)',\n          align: 'start',\n          sortable: false,\n          value: 'name',\n        },\n        { text: 'Calories', value: 'calories' },\n        { text: 'Fat (g)', value: 'fat' },\n        { text: 'Carbs (g)', value: 'carbs' },\n        { text: 'Protein (g)', value: 'protein' },\n        { text: 'Actions', value: 'actions', sortable: false },\n      ],\n      desserts: [],\n      editedIndex: -1,\n      defaultItem: {\n        name: '',\n        calories: 0,\n        fat: 0,\n        carbs: 0,\n        protein: 0,\n      },\n    }),\n\n    computed: {\n      ...mapState({\n        dialogState: state => state.dialog,\n        dialogDelState: state => state.dialogDelete,\n        currentItem: state => state.curItem,\n      }),\n      dialog: {\n        get () {\n          return this.dialogState\n        },\n        set (boolValue) {\n          return this.hideModalM(boolValue)\n        },\n      },\n      dialogDelete: {\n        get () {\n          return this.dialogDelState\n        },\n        set (boolValue) {\n          return this.hideModalDelM(boolValue)\n        },\n      },\n      curItem: {\n        get () {\n          return this.currentItem\n        },\n        set (item) {\n          if (item) {\n            return item\n          } else {\n            return this.defaultItem\n          }\n        },\n      },\n      ...mapGetters([\n        'tableItems',\n      ]),\n      ...mapFields({\n        fields: ['name', 'calories', 'fat', 'carbs', 'protein'],\n        base: 'item',\n        mutation: 'updateItem',\n      }),\n      formTitle () {\n        return this.editedIndex === -1 ? 'New Item' : 'Edit Item'\n      },\n    },\n    // created () {\n    //   this.initialize()\n    // },\n\n    methods: {\n      openDialog: () => {\n\n      },\n      ...mapMutations([\n        'hideModalM',\n        'hideModalDelM',\n        'deleteItemConfirm',\n      ]),\n      ...mapActions([\n        'setItems',\n        'openDialog',\n        'saveNewItem',\n        'deleteItem',\n      ]),\n      // initialize () {\n      // },\n    },\n  }\n</script>\n"],"sourceRoot":"src/views/dashboard/tables"}]}