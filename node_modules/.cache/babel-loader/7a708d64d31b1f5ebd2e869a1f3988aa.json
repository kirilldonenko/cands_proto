   state.barImage = payload\n    },\n    SET_DRAWER (state, payload) {\n      state.drawer = payload\n    },\n    setItems: (state, payload) => {\n      state.desserts = payload\n    },\n    deleteItem: (state, payload) => {\n      state.editedIndex = state.desserts.indexOf(payload)\n      state.editedItem = Object.assign({}, payload)\n      state.dialogDelete = true\n    },\n    openDialog: (state, payload) => {\n      state.editedIndex = state.desserts.indexOf(payload)\n      state.curItem = Object.assign({}, payload)\n      state.dialog = true\n    },\n    closeDialog: (state) => {\n      state.dialog = false\n    },\n    saveItem: (state, payload) => {\n      axios.post('http://localhost:8090/index.php', payload).then((response) => {\n        if (state.editedIndex > -1) {\n          Object.assign(state.desserts[state.editedIndex], payload)\n        } else {\n          state.desserts.push(payload)\n        }\n        state.curItem = Object.assign({}, state.defaultItem)\n        state.editedIndex = -1\n        state.dialog = false\n      }).catch(error => {\n        console.log(error)\n      })\n    },\n    editItem: (state, payload) => {\n      debugger\n      state.editedIndex = state.desserts.indexOf(payload)\n      Object.assign(state.desserts[state.editedIndex], payload)\n      },\n    deleteItemConfirm: (state, payload) => {\n      state.desserts.splice(state.editedIndex, 1)\n      state.dialogDelete = false\n    },\n    clearCurItem: (state) => {\n      state.curItem = state.defaultItem\n    },\n  },\n  actions: {\n    showModal: ({ commit }) => {\n      commit('showModal')\n    },\n    hideModal: ({ commit }) => {\n      commit('hideModalM')\n    },\n    hideModalDel: ({ commit }) => {\n      commit('hideModalDelM')\n    },\n    toggleDialog: ({ commit }) => {\n      commit('toggleDialog')\n    },\n    setItems: ({ commit }) => {\n        axios.get('http://localhost:8090/index.php').then((response) => {\n          commit('setItems', response.data)\n        }).catch(error => {\n          console.log(error)\n        })\n    },\n    async saveNewItem ({ dispatch, commit }, payload) {\n      await dispatch('saveItem', payload)\n      commit('clearCurItem')\n    },\n    saveItem: ({ commit }, payload) => {\n      commit('saveItem', payload)\n    },\n    deleteItem: ({ commit }, payload) => {\n      commit('deleteItem', payload)\n    },\n    async closeDialog ({ dispatch, commit }) {\n      await dispatch('saveNewItem')\n      commit('closeDialog')\n    },\n    openDialog: ({ commit }, payload) => {\n      commit('openDialog', payload)\n    },\n    updateItem: ({ commit }) => {\n      commit('updateItem')\n    },\n  },\n  getters: {\n    tableItems: state => state.desserts,\n  },\n})\n"]}]}